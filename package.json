{
    "name": "eboard",
    "version": "0.9.1",
    "author": "Heng Li",
    "description": "eboard lib",
    "license": "MIT",
    "engines": {
        "node": ">=6"
    },
    "dependencies": {
        "fabric": "^4.6.0",
        "lodash": "^4.17.21",
        "pdfjs-dist": "^2.9.359",
        "rc-react": "^1.1.1",
        "react-color": "^2.19.3",
        "react-custom-scrollbars": "^4.2.1",
        "react-pdf": "^5.3.2",
        "typings": "^2.1.1",
        "core-decorators": "^0.20.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "worker-loader": "^3.0.8",
        "lz-string": "^1.4.4",
        "easydropdown":"^4.2.0",
        "hammerjs": "^2.0.8"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:codyysh/eboard.git"
    },
    "scripts": {
        "watch-tsc": "rc-tools run watch-tsc",
        "build": "rc-tools run build",
        "dist": "rc-tools run dist",
        "gh-pages": "rc-tools run gh-pages",
        "start": "set NODE_ENV=development && node ./cli/open.js && rc-tools run server --a=1",
        "compile": "rc-tools run compile --babel-runtime",
        "pub": "rc-tools run pub --babel-runtime",
        "lint": "rc-tools run lint --no-js-lint",
        "karma": "rc-test run karma",
        "saucelabs": "rc-test run saucelabs",
        "test": "rc-test run test",
        "chrome-test": "rc-test run chrome-test",
        "coverage": "rc-test run coverage",
        "prepublish": "rc-tools run guard",
        "typings": "typings install fabric=github:codyysh/DefinitelyTyped/types/fabric/index.d.ts --save"
    },
    "files": [
        "lib",
        "es",
        "dist"
    ],
    "config": {
        "port": 8002,
        "entry":"./src/index.dist.ts"
    },
    "main": "lib/index.js",
    "module": "es/index.js",
    "typings": "lib/index.d.ts",
    "devDependencies": {
        "@types/hammerjs": "^2.0.40",
        "@types/lodash": "^4.14.172",
        "@types/pdfjs-dist": "^2.7.4",
        "@types/react-custom-scrollbars": "^4.0.8",
        "@types/perfect-scrollbar":"^1.3.0",
        "@types/lz-string":"^1.3.34",
        "@material-ui/core": "^4.12.3",
        "@material-ui/icons": "^4.11.2",
        "@types/classnames": "^2.3.1",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.7.13",
        "@types/react": "^17.0.20",
        "@types/react-dom": "^17.0.9",
        "@types/react-redux": "^7.1.18",
        "@types/react-router-config": "^5.0.3",
        "@types/react-router-dom": "^5.1.8",
        "@types/redux-devtools-extension": "^2.13.2",
        "@types/redux-thunk": "^2.1.0",
        "antd": "^4.16.13",
        "async": "^3.2.1",
        "babel-runtime": "^6.26.0",
        "bluebird": "^3.7.2",
        "classnames": "^2.3.1",
        "core-js": "^3.17.2",
        "jest": "^27.1.0",
        "open": "^8.2.1",
        "rc-tools":"^9.6.1-alpha.1",
        "react-redux": "^7.2.5",
        "react-router-config": "^5.1.1",
        "react-router-dom": "^5.3.0",
        "redux-devtools-extension": "^2.13.9",
        "redux-thunk": "^2.3.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-react": "^5.0.0",
        "typescript": "^4.4.2",
        "babel-polyfill": "^6.26.0",
        "lodash-decorators": "^6.0.1"
    }
}
