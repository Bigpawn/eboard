{
    "name": "eboard",
    "version": "0.9.30",
    "author": "Heng Li",
    "description": "eboard lib",
    "license": "MIT",
    "engines": {
        "node": ">=6"
    },
    "dependencies": {
        "fabric": "^2.6.0",
        "lodash": "^4.17.10",
        "pdfjs-dist": "^2.0.943",
        "rc-react": "^1.1.1",
        "react-color": "^2.14.1",
        "react-custom-scrollbars": "^4.2.1",
        "react-pdf": "^3.0.5",
        "typings": "^2.1.1",
        "worker-loader": "^2.0.0",
        "lz-string":"^1.4.4",
        "easydropdown":"^4.0.3",
        "hammerjs": "^2.0.8",
        "kxt-web": "^1.4.2",
        "core-decorators": "^0.20.0",
        "react": "^16.8.5",
        "react-dom": "^16.8.5"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:codyysh/eboard.git"
    },
    "scripts": {
        "watch-tsc": "rc-tools run watch-tsc",
        "build": "rc-tools run build",
        "dist": "rc-tools run dist",
        "gh-pages": "rc-tools run gh-pages",
        "start": "set NODE_ENV=development && node ./cli/open.js && rc-tools run server --a=1",
        "compile": "rc-tools run compile --babel-runtime",
        "pub": "rc-tools run pub --babel-runtime",
        "lint": "rc-tools run lint --no-js-lint",
        "karma": "rc-test run karma",
        "saucelabs": "rc-test run saucelabs",
        "test": "rc-test run test",
        "chrome-test": "rc-test run chrome-test",
        "coverage": "rc-test run coverage",
        "prepublish": "rc-tools run guard",
        "typings": "typings install fabric=github:codyysh/DefinitelyTyped/types/fabric/index.d.ts --save"
    },
    "files": [
        "lib",
        "es",
        "dist"
    ],
    "config": {
        "port": 8002,
        "entry":"./src/index.dist.ts"
    },
    "main": "lib/index.js",
    "module": "es/index.js",
    "typings": "lib/index.d.ts",
    "devDependencies": {
        "@types/hammerjs": "^2.0.36",
        "@types/lodash": "^4.14.110",
        "@types/pdfjs-dist": "^0.1.2",
        "@types/react-custom-scrollbars": "^4.0.4",
        "@types/perfect-scrollbar":"^1.3.0",
        "@types/lz-string":"^1.3.32",
        "@material-ui/core": "^1.3.1",
        "@material-ui/icons": "^1.1.0",
        "@types/classnames": "^2.2.4",
        "@types/jest": "^23.1.4",
        "@types/node": "^10.3.2",
        "@types/react": "^16.3.17",
        "@types/react-dom": "^16.0.6",
        "@types/react-redux": "^6.0.3",
        "@types/react-router-config": "^1.0.7",
        "@types/react-router-dom": "^4.2.7",
        "@types/redux-devtools-extension": "^2.13.2",
        "@types/redux-thunk": "^2.1.0",
        "antd": "^3.6.5",
        "async": "^0.9.0",
        "babel-runtime": "6.x",
        "bluebird": "~2.9.30",
        "classnames": "^2.2.6",
        "core-js": "^2.5.1",
        "jest": "^23.2.0",
        "open": "^0.0.5",
        "yanxlg-rc-tools":"^8.2.1",
        "react-redux": "^5.0.7",
        "react-router-config": "^1.0.0-beta.4",
        "react-router-dom": "^4.3.1",
        "redux-devtools-extension": "^2.13.5",
        "redux-thunk": "^2.3.0",
        "tslint-config-prettier": "^1.13.0",
        "tslint-react": "^3.6.0",
        "typescript": "^2.9.1",
        "babel-polyfill": "^6.26.0",
        "lodash-decorators": "^6.0.1"
    }
}
